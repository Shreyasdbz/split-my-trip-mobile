{
  "expo": {
    "name": "split-my-trip",
    "slug": "split-my-trip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F08080"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shreyassane.splitmytrip",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.945902506805-nrlatrg80k38iqm8ppt446sf1qgq1ksb"
        }
      },
      "googleServicesFile": "./GoogleService-Info.plist",
      "rerequireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
